{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "carobra.com"
        }
      ],
      "destination": "https://www.carobra.com/:path*",
      "permanent": true
    }
  ],
  "trailingSlash": true
}
